{
    "version": 2,
    "builds": [
        {
            "src": "html/home.html",
            "use": "@vercel/static"
        },
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/html/home.html"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}
