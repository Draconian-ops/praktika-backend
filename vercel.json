{
    "version": 2,
    "builds": [
        {"src": "index.js", "use": "@vercel/node"},
        {"src": "html/**", "use": "@vercel/static"},
        {"src": "js/**", "use": "@vercel/static"}
    ],
    "routes": [
        {"src": "/js/(.*)", "dest": "/js/$1"},
        {"src": "/html/(.*)", "dest": "/html/$1"},
        {"src": "/(.*)", "dest": "index.js"}
    ]
}